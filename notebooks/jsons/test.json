    {
        "type":"readers.las",
        "filename":"input.laz"
     },
     {
        "type": "filters.mongo",
        "expression": { "$and": [
            { "ReturnNumber": { "$gt": 0 } },
            { "NumberOfReturns": { "$gt": 0 } }
        ] }
    },
    {
        "type":"filters.dem",
        "raster":"/Users/zachkeskinen/Documents/ice-road-copters/test/dems/full.tif",
        "limits":"Z[20:35]"
    },
    {
        "type":"filters.outlier",
        "method":"statistical",
        "mean_k":12,
        "multiplier":2.2
    },
    {
      "type":"filters.range",
      "limits":"classification![7:7]"
    },
    {
        "type":"filters.smrf",
        "ignore":"NumberOfReturns[0:0], ReturnNumber[0:0]"
    },
    {
        "type":"filters.range",
        "limits":"Classification[2:2]"
    },
    {
        "type":"writers.las",
        "filename":"/Users/zachkeskinen/Documents/ice-road-copters/test/09_EXPORT/merged_out.laz"
     },
    {
        "type":"writers.gdal",
